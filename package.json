{
  "name": "sonos-nfc",
  "version": "0.1.0",
  "description": "Use NFC tags to control Sonos - a node app inspired by and compatible with hankhank10/vinylemulator, with better NFC reader compatibility",
  "main": "sonos_nfc.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start-all": "concurrently --kill-others \"npm start\" \"npm explore sonos-http-api -- npm start\"",
    "start": "node index.js",
    "prepare": "husky install",
    "format": "prettier --write . && git add -A ."
  },
  "author": "Ryan Olf",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "nfc-pcsc": "^0.8.1",
    "nfccard-tool": "^0.1.1",
    "node-fetch": "^3.3.0",
    "sonos-http-api": "git+https://github.com/jishi/node-sonos-http-api.git"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-transform-amd-to-commonjs": "^1.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.8.0"
  }
}
